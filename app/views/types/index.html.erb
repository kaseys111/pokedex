<div class="page-header">
  <h1>
    Type Strategy
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Type</th>
      </tr>

      <% @types.each do |type| %>
      <tr>
        <td><a href="/types/<%= type.id %>"><%= type.title.capitalize %></a></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
