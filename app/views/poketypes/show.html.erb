<div class="page-header">
  <h3>Poketype #<%= @poketype.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Pokemon</dt>
      <dd><%= @poketype.pokemon_id %></dd>

      <dt>Type</dt>
      <dd><%= @poketype.type_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/poketypes" class="btn btn-primary">
        Back
      </a>
      <a href="/poketypes/<%= @poketype.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_poketype/<%= @poketype.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
